<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <PropertyGroup>
    <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
  </PropertyGroup>

  <ItemGroup>
    <AssemblyAttribute Include="System.Reflection.AssemblyMetadata" Condition=" '$(GenerateCakeAssemblyMetadataAttributes)' != 'false' ">
      <_Parameter1>CakeTargetCakeVersion</_Parameter1>
      <_Parameter2>1.0.0-rc0001</_Parameter2>
    </AssemblyAttribute>
  </ItemGroup>

</Project>
