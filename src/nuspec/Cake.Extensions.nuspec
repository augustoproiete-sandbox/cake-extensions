<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/01/nuspec.xsd">
  <metadata>
    <id>Cake.Extensions.PoC</id>
    <version>0.0.1-local</version>
    <authors>C. Augusto Proiete &amp; Contributors</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/augustoproiete/cake-extensions</projectUrl>
    <description>Provides a default set of APIs for building an Cake extensions (e.g. add-ins, modules, etc.).</description>
    <icon>images\icon.png</icon>
    <iconUrl>https://raw.githubusercontent.com/augustoproiete/cake-extensions/master/asset/cake-extensions-nuget.png</iconUrl>
    <releaseNotes>https://github.com/augustoproiete/cake-extensions/releases</releaseNotes>
    <copyright>Copyright 2020 C. Augusto Proiete &amp; Contributors</copyright>
    <tags>cake cake-extensions cake-addin cake-module cake-build cake-contrib</tags>
    <repository type="git" url="https://github.com/augustoproiete/cake-extensions.git" />
    <dependencies>
      <group targetFramework="net5.0">
        <dependency id="Cake.Core" version="1.0.0-rc0002" include="All" />
        <dependency id="Cake.Common" version="1.0.0-rc0002" include="All" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="Cake.Core" version="1.0.0-rc0002" include="All" />
        <dependency id="Cake.Common" version="1.0.0-rc0002" include="All" />
      </group>
      <group targetFramework=".NETFramework4.6">
        <dependency id="Cake.Core" version="1.0.0-rc0002" include="All" />
        <dependency id="Cake.Common" version="1.0.0-rc0002" include="All" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="build\**" target="build" />
    <file src="buildMultiTargeting\**" target="buildMultiTargeting" />

    <file src="lib\_._" target="lib/net5.0" />
    <file src="lib\_._" target="lib/netstandard2.0" />
    <file src="lib\_._" target="lib/net46" />

    <file src="..\..\asset\cake-extensions-nuget.png" target="images\icon.png" />
  </files>
</package>
